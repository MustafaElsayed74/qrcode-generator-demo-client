{
    "buildCommand": "npm run build",
    "outputDirectory": "dist/qrcode-client/browser",
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "http://www.qrcode-generator.somee.com/api/:path*"
        },
        {
            "source": "/((?!.*\\.).*)",
            "destination": "/index.html"
        }
    ]
}